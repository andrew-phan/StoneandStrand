<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="formkey"/>
    <head>
        <title>Designer Manager</title>
    </head>
    <body>
        <referenceBlock name="menu">
            <action method="setActive">
                <argument name="itemId" xsi:type="string">Ss_Designer::designer</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="page.title">
            <action method="setPageTitle">
                <argument name="pageTitle" xsi:type="string">Designer Manager</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Ss\Designer\Block\Adminhtml\Designer" name="admin.block.banner.grid.container"/>
        </referenceContainer>
    </body>
</page>
